set_global_assignment -name FAMILY "Cyclone V"
set_global_assignment -name DEVICE 5CEBA4F23C7
set_global_assignment -name TOP_LEVEL_ENTITY i2c_test
set_global_assignment -name ORIGINAL_QUARTUS_VERSION 13.1
set_global_assignment -name PROJECT_CREATION_TIME_DATE "23:07:57  JUNE 24, 2017"
set_global_assignment -name LAST_QUARTUS_VERSION 13.1
set_global_assignment -name PROJECT_OUTPUT_DIRECTORY output_files
set_global_assignment -name MIN_CORE_JUNCTION_TEMP 0
set_global_assignment -name MAX_CORE_JUNCTION_TEMP 85
set_global_assignment -name ERROR_CHECK_FREQUENCY_DIVISOR 256
set_global_assignment -name EDA_SIMULATION_TOOL "ModelSim-Altera (VHDL)"
set_global_assignment -name EDA_OUTPUT_DATA_FORMAT VHDL -section_id eda_simulation
set_global_assignment -name PARTITION_NETLIST_TYPE SOURCE -section_id Top
set_global_assignment -name PARTITION_FITTER_PRESERVATION_LEVEL PLACEMENT_AND_ROUTING -section_id Top
set_global_assignment -name PARTITION_COLOR 16764057 -section_id Top


##need this line for active serial programming.
set_global_assignment -name STRATIX_CONFIGURATION_DEVICE EPCS4
##Or disable EPCS ID check "on" when you convert sof fie in the convert programming file advanced dialog.
##http://www.alteraforum.com/forum/showthread.php?t=51663


##pin asignment.
set_location_assignment PIN_M9 -to pi_base_clk

##global clock set.
set_instance_assignment -name GLOBAL_SIGNAL "GLOBAL CLOCK" -to pi_base_clk

##I/O pins.
set_location_assignment PIN_P22 -to pi_reset_n
set_location_assignment PIN_U7 -to pi_key[0]
set_location_assignment PIN_W9 -to pi_key[1]
set_location_assignment PIN_M7 -to pi_key[2]
set_location_assignment PIN_M6 -to pi_key[3]

set_location_assignment PIN_AA2 -to po_led[0]
set_location_assignment PIN_AA1 -to po_led[1]
set_location_assignment PIN_W2 -to po_led[2]
set_location_assignment PIN_Y3 -to po_led[3]
set_location_assignment PIN_N2 -to po_led[4]
set_location_assignment PIN_N1 -to po_led[5]
set_location_assignment PIN_U2 -to po_led[6]
set_location_assignment PIN_U1 -to po_led[7]
set_location_assignment PIN_L2 -to po_led[8]
set_location_assignment PIN_L1 -to po_led[9]

set_location_assignment PIN_U13 -to pi_sw[0]
set_location_assignment PIN_V13 -to pi_sw[1]
set_location_assignment PIN_T13 -to pi_sw[2]
set_location_assignment PIN_T12 -to pi_sw[3]
set_location_assignment PIN_AA15 -to pi_sw[4]
set_location_assignment PIN_AB15 -to pi_sw[5]
set_location_assignment PIN_AA14 -to pi_sw[6]
set_location_assignment PIN_AA13 -to pi_sw[7]
set_location_assignment PIN_AB13 -to pi_sw[8]
set_location_assignment PIN_AB12 -to pi_sw[9]

set_location_assignment PIN_T19 -to po_i2c_scl
set_location_assignment PIN_T17 -to po_i2c_sda

##project files




set_global_assignment -name VHDL_FILE i2c.vhd
set_global_assignment -name VHDL_FILE i2c_test.vhd
set_instance_assignment -name PARTITION_HIERARCHY root_partition -to | -section_id Top